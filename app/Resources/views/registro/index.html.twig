                <table id="example" class="table table-striped table-bordered" style="width:100%">
                    <thead>
                    <tr>
                        <th>Id</th>
                        <th>Nombre</th>
                        <th>email</th>
                        <th>Institucin</th>
                        <th>Carrera</th>
                        <th>Matutino</th>
                        <th>Vespertino</th>
                        <th>Vegetariana</th>
                        <th>Playera</th>
                        <th>Talla</th>
                        <th>Activo</th>
                        <th>&nbsp;</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for registro in registros %}
                        <tr>
                            <td><a href="{{ path('registro_edit', { 'id': registro.id, 'mail': registro.mail }) }}">{{ registro.id }}</a></td>
                            <td>{{ registro.paterno |title ~ ' ' ~ registro.materno |title ~ ' ' ~ registro.nombre |title }}</td>
                            <td>{{ registro.mail }}</td>
                            <td>{{ registro.institucion }}</td>
                            <td>{{ registro.carrera }}</td>
                            <td>{% for key,value in registro.actividadm if value %}
                                    {#{{ key |title}}#}
                                    {% set act = key|split('',9) %}
                                    {% set nombre = 'actividad'~act.1  %}
                                    {{ attribute(_context, nombre) }}
                                    <br>
                                {% endfor %}
                            </td>
                            <td>{% for key,value in registro.actividadv if value %}
                                    {% set act = key|split('',9) %}
                                    {% set nombre = 'actividad'~act.1  %}
                                    {{ attribute(_context, nombre) }}
                                    <br>
                                {% endfor %}
                            </td>
                            <td class="text-center">{% if registro.comida == true %}
                                    <span class="badge badge-success">Si</span>
                                {% else %}
                                    <span class="badge badge-danger">No</span>
                                {% endif %}
                            </td>
                            <td class="text-center">{% if registro.sexo == true %}
                                    <span class="badge badge-primary"><i class="far fa-check-square"></i> Hombre</span>
                                {% elseif registro.sexo is null %}
                                    <span class="badge badge-warning "><i class="fa fa-check-exclamation"></i> <i class="fa fa-exclamation-circle"></i> Pendiente</span>
                                {% elseif registro.sexo == false %}
                                    <span class="badge badge-info "><i class="far fa-check-square"></i> Mujer</span>
                                {% endif %}
                            </td>
                            <td class="text-center">{{ registro.playera }}</td>
                            <td class="text-center">{% if registro.activo == true %}
                                    <span class="badge badge-success">Si</span>
                                {% else %}
                                    <span class="badge badge-danger">No</span>
                                {% endif %}
                            </td>
                            <td class="text-center">
                                <a href="{{ path('registro_edit', { 'id': registro.id, 'mail': registro.mail }) }}"><i class="far fa-edit"></i></a>
                            </td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>




