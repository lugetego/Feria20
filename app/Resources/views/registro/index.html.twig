            <script type="text/javascript" class="init">
                $(document).ready(function() {
                    $('#example').DataTable ( {
                        lengthChange: false,
                        pageLength: 50,
                        dom: 'Bfrtip',

                        buttons: [
                            {
                                text: '<i class="far fa-copy"></i>',
                                titleAttr: 'Copiar',
                                extend: 'copyHtml5',
                                exportOptions: {
                                    columns: ':visible'
                                }
                            },
                            {
                                extend: 'csvHtml5',
                                text: '<i class="fas fa-file-csv"></i>',
                                titleAttr: 'CSV separado por comas',
                                exportOptions: {
                                    columns: ':visible'
                                },
                                fieldSeparator: ',',
                                extension: '.csv'
                            },

                            // {
                            //     extend: 'pdfHtml5',
                            //     text: '<i class="far fa-file-pdf"></i>',
                            //     titleAttr: 'PDF',
                            //     exportOptions: {
                            //         columns: ':visible'
                            //     }
                            // },
                            {
                                extend:'colvis',
                                text: 'Columnas'
                            }

                        ],
                        "pagingType": "full_numbers",
                        "language": {
                            buttons: {
                                copyTitle: 'Copiado al portapapeles',
                            },
                            url: '//cdn.datatables.net/plug-ins/380cb78f450/i18n/Spanish.json'
                        }
                    } );

                } );
            </script>
        {% endblock %}

