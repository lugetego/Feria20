                                        {% endif %}
                                    {% endfor %}
                                    {% set mujeres = 0 %}
                                    {% set hombres = 0 %}
                                    {% for registro in registros %}
                                        {% if registro.sexo == 0 and registro.sexo is not null %}
                                            {% set mujeres = mujeres + 1 %}
                                        {% elseif registro.sexo == 1 and registro.sexo is not null %}
                                            {% set hombres = hombres + 1 %}
