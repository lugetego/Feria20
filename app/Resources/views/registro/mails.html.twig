                                        {% set actividad1mailv = totalv.actividad1mail | split(',') %}
                                        {% set mails1mv = actividad1mailm|merge(actividad1mailv) %}
                                        {% set mails1 = [] %}
                                        {% for mail in mails1mv %}
                                                {% if mail not in mails1 %}
                                                        {% set mails1 = mails1|merge([mail]) %}
                                                {% endif %}
                                        {% endfor %}
                                        {% for mail in mails1 if mail %}
                                                {% if loop.first == false  %},{% endif%}
                                                {{ mail }}
                                        {% endfor %}
